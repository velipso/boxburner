{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "composite": true,
    "allowJs": false,
    "noEmit": true,
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "outDir": "./dist"
  },
  "include": ["src"]
}
